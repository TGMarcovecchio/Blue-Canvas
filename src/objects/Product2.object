    <fields>
        <fullName>Prefix__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 

/*Special Exception to convert 1014 to 1026 */ 

AND(LEFT(ProductCode,1)=&quot;1&quot;, 
LEFT(ProductCode,4)=&quot;1014&quot;),&quot;1026&quot;, 

/*For all other Mattresses*/ 

IF( 
AND(LEFT(ProductCode,1)=&quot;1&quot;, 
LEFT(ProductCode,4) &lt;&gt; &quot;1014&quot;), LEFT(ProductCode,4), 

/*For all other Item Groups*/ 

LEFT(ProductCode,3)))</formula>
        <label>Prefix</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>

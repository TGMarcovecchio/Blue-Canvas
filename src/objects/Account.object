    <validationRules>
        <fullName>Enforce_Consultation_Fee_Percentage</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(CONTAINS(RecordType.Name, &quot;Vendor&quot;)),
NOT(ISBLANK(TEXT(Referral_Type__c))),
OR(NOT(Referral_Percent__c &gt;= 0), ISNULL(Referral_Percent__c)))</errorConditionFormula>
        <errorDisplayField>Referral_Percent__c</errorDisplayField>
        <errorMessage>Please Ensure that the Partner Consulation Fee is Equal to or Greater than Zero for Referral Partners</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enforce_Valid_Referral_Grandparent</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Setup.WS_Custom_Admin_Settings__c.Deactivate_Validation_Rules__c = FALSE,
NOT(ISBLANK( Grand_ParentAccount__c )), 
OR(ISNULL(Grand_ParentAccount__r.Grandparent_Referral_Percent__c ), 
ISBLANK(Grand_ParentAccount__r.Grandparent_Referral_Percent__c)))</errorConditionFormula>
        <errorDisplayField>Grand_ParentAccount__c</errorDisplayField>
        <errorMessage>You must provide a &quot;IsParent Consultant Fee&quot; on this Account before they can be referenced on related Referral Partners</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enforce_Valid_Referral_Parent</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Setup.WS_Custom_Admin_Settings__c.Deactivate_Validation_Rules__c = FALSE,
NOT(ISBLANK( ParentId )),
OR(ISNULL(Parent.Parent_Referral_Percent__c),
ISBLANK(Parent.Parent_Referral_Percent__c)))</errorConditionFormula>
        <errorDisplayField>ParentId</errorDisplayField>
        <errorMessage>You must provide a &quot;IsParent Consultant Fee&quot; on this Account before they can be referenced on related Referral Partners</errorMessage>
    </validationRules>

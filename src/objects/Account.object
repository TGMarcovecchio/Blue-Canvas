        <fullName>PBSI__Create_Sales_Order</fullName>
        <availability>online</availability>
        <description>Allows creation of a sales order from Account details page</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Create Sales Order</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&quot;/js/functions.js&quot;)}
{!requireScript(&quot;/soap/ajax/13.0/connection.js&quot;)}
{!requireScript(&quot;/soap/ajax/13.0/apex.js&quot;)}

try
{
        var straccountID = &quot;{!Account.Id }&quot;;

        var result =  sforce.apex.execute(&quot;PBSI.WebServiceClass&quot;, &quot;ConvertAccountInSalesOrder&quot;, {accountID: straccountID});
        var resultMsg = new String(result);
        var failureIndex = resultMsg.indexOf(&quot;Failure&quot;);

         if (failureIndex  != -1)
                 alert(result);
         else
        {
               var pageRef = &quot;/&quot; + result;
              var newwindow = window.open(pageRef,&apos;_self&apos;); 

              
       }
}
catch(e)
{
          alert(&quot;Failed to create Sales Order &quot; + e);
}</url>
    </webLinks>

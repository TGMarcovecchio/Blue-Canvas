    <validationRules>
        <fullName>Enforce_Medical_Type</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
OR(  
AND(NOT(ISBLANK(PBSI__Sales_Order__c)),
CONTAINS(TEXT(LeadSource), &quot;Medical&quot;),
ISBLANK(TEXT(Medical_Type__c)))
,
OR(AND(NOT(ISBLANK(PBSI__Sales_Order__c)),
CONTAINS(TEXT(Campaign.Type), &quot;Medical&quot;),
ISBLANK(TEXT(Medical_Type__c))),
AND(NOT(ISBLANK(PBSI__Sales_Order__c)),
CONTAINS(TEXT(Secondary_Campaign__r.Type), &quot;Medical&quot;),
ISBLANK(TEXT(Medical_Type__c)))))
,TRUE,FALSE)</errorConditionFormula>
        <errorDisplayField>Medical_Type__c</errorDisplayField>
        <errorMessage>Please provide a value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enforce_Valid_Lead_Source_Values</fullName>
        <active>true</active>
        <description>This rule was put in place at the request of Tom Hildt to ensure legacy lead source values were not used moving forward.</description>
        <errorConditionFormula>IF(OR(
TEXT(LeadSource) = &quot;Affiliate&quot;,
TEXT(LeadSource) = &quot;Blogger Program&quot;,
TEXT(LeadSource) = &quot;Business Benefit&quot;,
TEXT(LeadSource) = &quot;Customer Referral&quot;,
TEXT(LeadSource) = &quot;Hospitality&quot;,
TEXT(LeadSource) = &quot;Medical Program&quot;,
TEXT(LeadSource) = &quot;Medical Show&quot;,
TEXT(LeadSource) = &quot;Consumer Show&quot;,
TEXT(LeadSource) = &quot;Direct Sales&quot;,
TEXT(LeadSource) = &quot;Digital Media&quot;,
TEXT(LeadSource) = &quot;Store - City Creek, Utah&quot;,
TEXT(LeadSource) = &quot;Store - Factory, Utah&quot;,
TEXT(LeadSource) = &quot;Store - Orem, Utah&quot;,
TEXT(LeadSource) = &quot;Store - Salt Lake City, Utah&quot;,
TEXT(LeadSource) = &quot;Wholesale&quot;), 
FALSE, TRUE)</errorConditionFormula>
        <errorDisplayField>LeadSource</errorDisplayField>
        <errorMessage>Please choose a current Lead Source</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_SP1_to_Close</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBlank(Sales_Person_1__c), TEXT(StageName)=&quot;Closed Won&quot;)</errorConditionFormula>
        <errorMessage>Please provide the Sales Person 1, and Sales Person 2 if applicable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sales_Order_Reference_Not_Changable</fullName>
        <active>true</active>
        <description>Users not allowed to change Sales Order Reference After it is Set</description>
        <errorConditionFormula>AND(
PriorValue( PBSI__Sales_Order__c )&lt;&gt; NULL,
ISChanged(PBSI__Sales_Order__c ),
PBSI__Sales_Order__c  &lt;&gt; PriorValue(PBSI__Sales_Order__c )
)</errorConditionFormula>
        <errorDisplayField>PBSI__Sales_Order__c</errorDisplayField>
        <errorMessage>You are not allowed to change the Sales Order field on Opportunity once a Sales Order has been generated for the Opportunity.</errorMessage>
    </validationRules>

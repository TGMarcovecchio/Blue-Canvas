    <fields>
        <fullName>Close_Month__c</fullName>
        <externalId>false</externalId>
        <formula>CASE(
  MONTH( CloseDate ),
  1, &quot;01. January&quot;,
  2, &quot;02. February&quot;,
  3, &quot;03. March&quot;,
  4, &quot;04. April&quot;,
  5, &quot;05. May&quot;,
  6, &quot;06. June&quot;,
  7, &quot;07. July&quot;,
  8, &quot;08. August&quot;,
  9, &quot;09. September&quot;,
  10, &quot;10. October&quot;,
  11, &quot;11. November&quot;,
  &quot;12. December&quot;
)</formula>
        <inlineHelpText>Month Label from Close Date</inlineHelpText>
        <label>Close Month</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Close_Weekday__c</fullName>
        <externalId>false</externalId>
        <formula>CASE(
  MOD( CloseDate  - DATE( 1900, 1, 7 ), 7 ),
  0, &quot;Sunday&quot;,
  1, &quot;Monday&quot;,
  2, &quot;Tuesday&quot;,
  3, &quot;Wednesday&quot;,
  4, &quot;Thursday&quot;,
  5, &quot;Friday&quot;, 
  &quot;Saturday&quot;
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Close Weekday</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Commissionable_Amount__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
OR(ISNULL(Sales_Person_2__c),
   ISBLANK(Sales_Person_2__c)),
Net_Revenue__c ,
Net_Revenue__c * 0.5)</formula>
        <label>Commissionable Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Con_CHNL__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
OR(ISPICKVAL(LeadSource, &quot;Medical Program&quot;),
   ISPICKVAL(LeadSource, &quot;Medical Show&quot;)),&quot;MEDICAL&quot;,

IF(ISPICKVAL(LeadSource, &quot;Customer Referral&quot;),&quot;CUSTOMER REF&quot;,

IF(ISPICKVAL(LeadSource, &quot;Hospitality&quot;),&quot;HOSPITALITY&quot;,

IF(
OR(ISPICKVAL(LeadSource, &quot;Affiliate&quot;), 
   ISPICKVAL(LeadSource, &quot;Business Benefit&quot;),
   ISPICKVAL(LeadSource, &quot;Business Referral&quot;)),&quot;3rd PARTY AFFILIATE&quot;,

IF(
OR(ISPICKVAL(LeadSource, &quot;Store - Draper, Utah&quot;), 
   ISPICKVAL(LeadSource, &quot;Store - Orem, Utah&quot;),
   ISPICKVAL(LeadSource, &quot;Store - Factory, Utah&quot;),
   ISPICKVAL(LeadSource, &quot;Store - Salt Lake City, Utah&quot;)),&quot;STORES&quot;,

IF(
OR(ISPICKVAL(LeadSource, &quot;Direct Sales&quot;),
   ISPICKVAL(LeadSource, &quot;Digital Media&quot;),
   ISPICKVAL(LeadSource, &quot;Consumer Show&quot;)),&quot;DIRECT&quot;,

IF(ISPICKVAL(LeadSource, &quot;Blogger Program&quot;),&quot;BLOGGER&quot;,

IF(ISPICKVAL(LeadSource, &quot;Wholesale&quot;),&quot;WHSLE&quot;,

&quot;&quot;))))))))</formula>
        <label>Con. CHNL</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>

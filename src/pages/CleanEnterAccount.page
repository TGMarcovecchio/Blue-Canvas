<apex:page standardController="Account">
  <html>
    <head>
      <script src="https://s7.activeprime.com/shared/js/jquery.min.js" type="text/javascript"></script>
      <script>
        ap$(document).ready(function(){
          var base_url = 'https://s7.activeprime.com/';
          var key = 'worldclassindustrialnetwork__meja_5543_mexu';
          var session=codec.encode('{!$User.Username}_session_{!$Api.Session_ID}');
          var url = base_url + 'ce/action/enter/account/ui/key=' + key + '/?returnwindow=_self&session=' + session;
          
          var record_type = get_url_param('RecordType')[0];
          var ent = get_url_param('ent')[0];
          var search_param = get_url_param('search_param')[0];
          if (search_param != null){
            search_param = decodeURIComponent(search_param);
            url = base_url + 'aps/action/search/ui/key=' + key + '/?returnwindow=_self&session=' + session + search_param;
          }else{
            if (record_type != null){
              url = url + '&RecordType=' + record_type;
            }
            if (ent != null){
              url = url + '&ent=' + ent;
            }
          }
          
          var _scrolling = "auto";
          if(ap$.pm.browser_supports()) {
            _scrolling = "no";
          }
          ap$('#ap_body').prepend('<iframe id="ap_window" src="'+url+'" width="100%" height="100%" scrolling="'+_scrolling+'" frameborder="0"/>');
  
          if(ap$.pm.browser_supports()) {
            ap$('#ap_body').height(630);
            ap$('#sidebarDiv').height(630);
            ap$('#ap_window').height(600);
            ap$.pm.bind("ap_resize_iframe", function(data) {
              var h = Math.max(data['height'],650);
              ap$('#ap_body').height(h+30);
              ap$('#sidebarDiv').height(h+30);
              ap$('#ap_window').height(h);
            });
          }
          else {
            ap$('#ap_body').height(2030);
            ap$('#sidebarDiv').height(2030);
            ap$('#ap_window').height(2000);
          }
        });
      </script>
    </head>
    <body>
      <div id="ap_body"></div>
    </body>
  </html>
</apex:page>
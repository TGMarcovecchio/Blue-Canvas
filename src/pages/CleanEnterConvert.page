    function viewAccount() {
      var acc_id = jQuery('#accid').val();
      if (acc_id == null || acc_id == "" || acc_id == "--None--" || acc_id == 000000000000000) {
        alert("You can only view existing accounts.");
      } else if (sfdc_url){
        loc = sfdc_url + acc_id + '/p';
        window.open(loc,'APLeadConversion');
      } else {
        printWin('/'+acc_id+'/p');
      }
    }
    
    function viewContact() {
      var con_id = jQuery('#conid').val();
      if (con_id == null || con_id == "" || con_id == "--None--" || con_id == 000000000000000) {
        alert("You can only view existing contacts.");
      } else if (sfdc_url){
        loc = sfdc_url + con_id + '/p';
        window.open(loc,'APLeadConversion');
      } else {
        printWin('/'+con_id+'/p');
      }
    }
    
    // shows the Overwrite Lead Source checkbox when a an existing person acct is selected..
    function accountSelChange(selId) {
      var sel = document.getElementById(selId);
      var overwriteDiv = document.getElementById('overwritediv');
      if (sel.value != null && sel.value != '' && sel.value != '000000000000000') {
        overwriteDiv.style.display='';
      } else {
        overwriteDiv.style.display='none';
      }
    }
